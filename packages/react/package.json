{
  "name": "@ajmchambers/components-react",
  "private": false,
  "version": "1.0.4",
  "description": "Hopefully a collection of lit components wrapped for react",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "files": [
    "/index.{d.ts,d.ts.map,js,js.map}",
    "/my-element.{d.ts,d.ts.map,js,js.map}",
    "/my-button.{d.ts,d.ts.map,js,js.map}",
    "/src/",
    "!/src/test/"
  ],
  "exports": {
    ".": "./index.js",
    "./my-element.js": "./my-element.js",
    "./my-button.js": "./my-button.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf my-element.{d.ts,d.ts.map,js,js.map} test/my-element.{d.ts,d.ts.map,js,js.map} test/my-element_test.{d.ts,d.ts.map,js,js.map}"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "author": "Google LLC",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@ajmchambers/components-core": "^1.0.3",
    "@lit-labs/react": "^1.0.0-rc.2"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/react": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "@web/dev-server": "0.0.29",
    "@web/dev-server-legacy": "^0.1.4",
    "@web/dev-server-rollup": "^0.2.7",
    "eslint": "^6.8.0",
    "prettier": "^2.0.4",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.2",
    "rollup-plugin-summary": "^1.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^3.8.3"
  }
}
